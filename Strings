     char ch[] ={'j','a','v', 'a','p','o','i','n','t'};
      String p = new String(ch);
       
       // comparison according to dictionary
      String name2="hello";
      String name3="wello";
      if(name2.compareTo(name3)==0){
          System.out.println("strings are equal");
      }
      else if(name2.compareTo(name3)>0){
          System.out.println("string 1 is greater");
      }
      else{
          System.out.println("string 2 is greater");
      }
      System.out.print(name2.equals(name3));
	
	
	//to print substrings
