	        System.out.println(name);
			String name1 = sc.nextLine(); // how to take input a sentence 
		System.out.println(name1);
		//how to concatinate the words and letters
		String fullname = name +" "+ name1;
		System.out.println("my name is  " + fullname);
		
		 String s ="This is My name";
		 System.out.println(s.length());
		System.out.println(s.charAt(4));
		System.out.println(s.indexOf("e"));
		System.out.println(s.indexOf("e",5));
		System.out.println(s.indexOf("my",7));
		System.out.println(s.toUpperCase());
		System.out.println(s.toLowerCase());
		System.out.println(Character.toUpperCase('a'));
		System.out.println(s.substring(3));
		System.out.println(s.substring(1,3));
		System.out.println(s.replace('e','f'));
		System.out.println(s.replace("my","oo"));
		
		
		 // how to split the words
		 String ret[] =s.split(" ");
		 for(int i=0;i<ret.length;i++){
		     System.out.println(ret[i]);
		 }
		System.out.println(s);
		//to print letters of a sentence and words
		for(int i=0;i<s.length();i++){
		    System.out.println(s.charAt(i));
		}
		
		
