import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
    // if condition (there is no need to write else condition with if condition)
    int a sc.nextInt();
    int b =sc.nextInt();
    if(a==b){
    System.out.println(true);
    }
    else{
    System.out.println(false);
    }
    //if-else if(only one condition runs which is true and no onther condition check after that)
     if(a==b){
    System.out.println(true);
    }
    else if(a!=b){
    System.out.println(false);
    }
    else if(a>b){
    System.out.println("a is greater");
    }
    else{
    System.out.println("b is greater");
    }
    //if if if (all the condition runs which one is true)
     if(a==b){
    System.out.println(true);
    }
     if(a!=b){
    System.out.println(false);
    }
     if(a>=b){
    System.out.println("a is greater");
    }
    // switch case 
    int month =sc.nextInt();
    switch(month){
         case 1:
         case 3:
         case 5:
         case 7:
         case 8:
         case 10:
         case 12: 
             System.out.println("31");
             break;
        case 4:
        case 6:
        case 9:
        case 11:
           System.out.println("30");
           break;
        case 2:
           System.out.println("28");
           break;
        default:
            System.out.println("wrong number entered");
            // we can further discuss about break statement
	}
}
