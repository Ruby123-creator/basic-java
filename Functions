import java.util.*;
public class Main
{

		
         public static void main(String[] args){
          Scanner sc = new Scanner(System.in);
          int m = sc.nextInt();
          int n = sc.nextInt();
          int fin_add = sum( m ,n);
          System.out.println(fin_add);
      }
      public static int sum(int a , int b){
          int add = a+b;
          System.out.println(difference(a,b));
          return add;
      }
      public static int difference(int a , int b){
          int sub = a-b;
          return sub;
      }
   }
   



import java.util.*;
public class Main
{
public static void main(String[] args){
        System.out.println("hello to main ");
        sumofnumbers(5);
        f1();
     }
    
    public static void sumofnumbers(int n){
        int sum =0;
        for(int i=1;i<=n;i++){
            sum += i;
        }
        System.out.println(sum);
        return;
    }
    public static void f1(){
        System.out.println("enter to f1");
        int x = f2();
        System.out.println(x);
        System.out.println("after executing f2");
    }
    public static int f2(){
        System.out.println("enter to f2");
        f3();
        System.out.println(" after executing f3");
        return 5;
    }
    public static void f3(){
        System.out.println("enter to f3 and return from where it calls");
    }
    
 }   
 
 
 
 
 import java.util.*;
public class Main{
    

    public static void main(String[] args){
        int a =5;
        a = increment(a);
        System.out.println(a);
        int []arr ={1,2,3,4,5};
        System.out.println(Arrays.toString(arr));
       
        arrayincrement(arr);
        System.out.println(Arrays.toString(arr));
         for(int i=0;i<arr.length;i++){
            System.out.println(arr[i]);
        }
    }
    public static int increment(int x){
        x++;
        System.out.println(x);
        return x;
    }
    public static void arrayincrement(int x[]){
        for(int i=0;i<x.length;i++){
            x[i]++;
        }
        
    }
       

}
